{
    "name": "proactif",
    "version": "1.0.0",
    "private": true,
    "devDependencies": {
        "svgo": "^3.1.0",
        "svg-sprite": "^1.5.0"
    },
    "scripts": {
        "icons:svgo": "svgo -f icons -o icons/optimized",
        "icons:sprite": "svg-sprite --symbol --symbol-dest icons --symbol-sprite sprite.min.svg icons/optimized/*.svg",
        "icons:build": "npm run icons:svgo && npm run icons:sprite"
    }
}